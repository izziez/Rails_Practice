<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @artist.name %>
</p>

<p>
  <strong>Genre:</strong>
  <%= @artist.genre_id %>
</p>

<p>
  <strong>Songs by this artist</strong>
    <ul>
      <% @artist.songs.each do |song| %>
      <li><%= song.name %></li>
      <% end %>
    </ul>

</p>

<%= link_to 'Edit', edit_artist_path(@artist) %> |
<%= link_to 'Back', artists_path %>
